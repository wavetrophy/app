  "scripts": {
    "ng": "ng",
    "start": "ionic serve -l --devapp",
    "build": "npm run android:build && npm run ios:build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "purge": "npm run clear && npm run android:add && npm run ios:add",
    "clear": "rm -rf node_modules www platforms plugins && npm install",
    "android": "cp ./resources/android/project.properties platforms/android && ionic cordova run android -l --debug --device",
    "android:emulate": "ionic cordova emulate android -l --debug",
    "android:add": "ionic cordova platform add android && npm run android:build",
    "android:resources": "ionic cordova resources android -sif",
    "android:build": "cp ./resources/android/project.properties platforms/android && ionic cordova build android --prod",
    "android:build:prod": "sh ./bin/build-android.sh",
    "android:clear": "ionic cordova platform rm android && npm run android:add",
    "ios": "ionic cordova run ios -l --debug --buildConfig ./build.json",
    "ios:emulate": "ionic cordova emulate ios -l --debug --target='iPhone-X,com.apple.CoreSimulator.SimRuntime.iOS-12-2' --buildConfig ./build.json",
    "ios:emulate:prod": "ionic cordova emulate ios --prod --release --target='iPhone-X,com.apple.CoreSimulator.SimRuntime.iOS-12-2' --buildConfig ./build.json",
    "ios:emulate:iphone-x": "ionic cordova emulate ios --prod --release --target='iPhone-XS-Max,com.apple.CoreSimulator.SimRuntime.iOS-12-2' --buildConfig ./build.json",
    "ios:emulate:iphone-5": "ionic cordova emulate ios --prod --release --target='iPhone-6s-Plus,com.apple.CoreSimulator.SimRuntime.iOS-12-2' --buildConfig ./build.json",
    "ios:emulate:ipad-pro-2": "ionic cordova emulate ios --prod --release --target='iPad-Pro--12-9-inch---2nd-generation-,com.apple.CoreSimulator.SimRuntime.iOS-12-2' --buildConfig ./build.json",
    "ios:emulate:ipad-pro-3": "ionic cordova emulate ios --prod --release --target='iPad-Pro--12-9-inch---3rd-generation-,com.apple.CoreSimulator.SimRuntime.iOS-12-2' --buildConfig ./build.json",
    "ios:emulate:all": "sh ./bin/emulate-all.sh",
    "ios:emulate:": "ionic cordova emulate ios --prod --release --target='iPhone-6s-plus,com.apple.CoreSimulator.SimRuntime.iOS-12-2' --buildConfig ./build.json",
    "ios:add": "ionic cordova platform add ios && npm run ios:build",
    "ios:resources": "ionic cordova resources ios -sif",
    "ios:build": "ionic cordova build ios --prod --release --buildConfig ./build.json",
    "ios:build:prod": "ionic cordova build ios --prod --release --buildConfig ./build.json",
    "ios:clear": "ionic cordova platform rm ios && npm run ios:add"
  },
